{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"]
    }
  ],
  "baseBranches": ["main"],
  "automerge": true,
  "automergeType": "pr",
  "prConcurrentLimit": 10,
  "major": {
    "automerge": false
  },
  "labels": ["dependencies", "renovate"]
}
