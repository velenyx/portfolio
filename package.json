{
  "name": "velenyx-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=3.0.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "npx jest",
    "test:unit:watch": "npx jest --watch",
    "prettier:check": "npx prettier --check \"**/*.{ts,tsx,css,mdx}\"",
    "prettier:fix": "npx prettier --write \"**/*.{ts,tsx,css,mdx}\"",
    "lint:eslint:check": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:eslint:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "postinstall": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.6",
